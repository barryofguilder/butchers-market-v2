<EmberFlatpickr
  @allowInput={{@allowInput}}
  @appendDataInput={{@appendDataInput}}
  @date={{@date}}
  @dateFormat={{@dateFormat}}
  @disableMobile={{true}}
  @enableTime={{@enableTime}}
  @noCalendar={{@noCalendar}}
  @onChange={{@onChange}}
  @time_24hr={{@time_24hr}}
  data-test-id="datepicker"
  class={{this.inputClasses}}
  ...attributes
/>
