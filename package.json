{
  "name": "butchers-market",
  "version": "4.0.0",
  "private": true,
  "description": "Source code for The Butcher's Market website.",
  "repository": "https://github.com/stewartandrewmiller/butchers-market-v2",
  "license": "MIT",
  "author": "Jason Barry <jbarry@hey.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "NODE_ENV=production TAILWIND_MODE=build ember build --environment=production",
    "deploy": "ember deploy production --activate",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\" --prefixColors auto && npm run format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "prettier": "prettier --write \"**/*.{css,gjs,gts,hbs,html,js,json,ts}\"",
    "start": "TAILWIND_MODE=watch ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\" --prefixColors auto",
    "test:ember": "ember test"
  },
